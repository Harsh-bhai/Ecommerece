{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "slug": {
      "type": "uid",
      "minLength": 3,
      "required": true
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "size": {
      "type": "string",
      "maxLength": 70
    },
    "price": {
      "type": "integer",
      "default": 0
    },
    "ogprice": {
      "type": "integer",
      "required": true
    },
    "availableqty": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "descriptionmore": {
      "type": "text"
    },
    "category": {
      "type": "string",
      "required": true
    }
  }
}
